{
  "presets": [
    "next/babel",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10.14",
          "browsers": ["last 2 versions", "> 5%"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    [
      "inline-react-svg",
      {
        "svgo": {
          "cleanupNumericValues": { "floatPrecision": 2 },
          "removeTitle": true
        }
      }
    ]
  ]
}
